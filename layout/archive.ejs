<div id="archive">
  <ul class="post-list">
    <% var year = 0 %>
    <% var change = false %>
    <% page.posts.sort('title', 'asc').each(function(post) { %>
      <li class="post-item">
        <div id="<%- post.name %>"><%- post.content %><div>
      </li>
    <% }); %>
  </ul>
  <%- partial('_partial/pagination') %>
</div>